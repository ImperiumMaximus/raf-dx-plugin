{
  "name": "raf-dx-plugin",
  "version": "0.0.1",
  "author": "Raffaele Fioratto",
  "bugs": "https://github.com/ImperiumMaximus/raf-dx-plugin/issues",
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2.1.3",
    "@salesforce/core": "^2.1.5",
    "child-process-promise": "^2.2.1",
    "cli-progress": "^3.8.2",
    "csv": "^5.3.2",
    "csvtojson": "^2.0.10",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "intercept-stdout": "^0.1.2",
    "json-query": "^2.2.2",
    "lodash": "^4.17.20",
    "pino": "^6.1.1",
    "pino-pretty": "^4.0.0",
    "salesforce-alm": "^48.8.0",
    "through2": "^4.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/ts-sinon": "^1.2.3",
    "@types/chai": "^4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "globby": "^8",
    "mocha": "^8",
    "nyc": "^15",
    "ts-node": "^8",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ImperiumMaximus/raf-dx-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "raf:package": {
        "description": "Helper commands for managed package"
      },
      "raf:package:migrate": {
        "description": "Migrate a set of installed packages from an Org (Sandbox or Scratch) to another Org (Sandbox or Scratch)"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ImperiumMaximus/raf-dx-plugin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
