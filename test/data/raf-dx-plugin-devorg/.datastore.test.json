{
  "pull": [
    {
      "fields": [
        "Name",
        "Type",
        "Phone",
        "Website",
        "ExternalId__c"
      ],
      "object": "Account",
      "where": "ExternalId__c != null",
      "order": "ExternalId__c",
      "storeIn": "test/data/accounts.csv"
    }
  ],
  "push": [
    {
      "object": "Account",
      "operation": "upsert",
      "externalId": "ExternalId__c",
      "source": "test/data/accounts.csv"
    }
  ]

}
